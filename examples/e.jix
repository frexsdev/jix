%include "./examples/natives.hix"
    
    jmp main
main:
    push 1.
    push 1.
    push 1.
loop:
    push 1.
    dup 2
    div
    add

    swap 2
    push 1.
    add
    dup 0
    swap 2
    mult

    swap 1
    swap 2

    dup 2
    push 100.
    lt

    jmp_if loop

    swap 1
    drop
    swap 1
    drop

    native print
    
    halt
